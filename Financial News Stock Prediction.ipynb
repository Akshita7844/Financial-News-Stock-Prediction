    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Lists to store results\n",
    "thresholds = np.arange(0.5, 0.96, 0.05)\n",
    "accuracies = []\n",
    "prediction_counts = []\n",
    "\n",
    "# Loop through thresholds\n",
    "for threshold in thresholds:\n",
    "    def predict_with_dynamic_threshold(pos_prob, neg_prob):\n",
    "        if pos_prob >= threshold:\n",
    "            return 'up'\n",
    "        elif neg_prob >= threshold:\n",
    "            return 'down'\n",
    "        else:\n",
    "            return 'neutral'\n",
    "\n",
    "    final_df_lagged['Predicted_Change'] = final_df_lagged.apply(\n",
    "        lambda row: predict_with_dynamic_threshold(row['Pos_Prob'], row['Neg_Prob']),\n",
    "        axis=1\n",
    "    )\n",
    "    \n",
    "    filtered_df = final_df_lagged[final_df_lagged['Predicted_Change'] != 'neutral']\n",
    "    accuracy = (filtered_df['Predicted_Change'] == filtered_df['Lagged_Price_Change']).mean()\n",
    "    \n",
    "    accuracies.append(accuracy if not np.isnan(accuracy) else 0)\n",
    "    prediction_counts.append(len(filtered_df))\n",
    "\n",
    "# Plot accuracy vs threshold\n",
    "plt.figure(figsize=(12, 5))\n",
    "\n",
    "plt.subplot(1, 2, 1)\n",
    "plt.plot(thresholds, accuracies, marker='o')\n",
    "plt.title('Threshold vs Accuracy')\n",
    "plt.xlabel('Threshold')\n",
    "plt.ylabel('Accuracy')\n",
    "plt.grid(True)\n",
    "\n",
    "# Plot prediction count vs threshold\n",
    "plt.subplot(1, 2, 2)\n",
    "plt.plot(thresholds, prediction_counts, marker='o', color='orange')\n",
    "plt.title('Threshold vs Number of Predictions')\n",
    "plt.xlabel('Threshold')\n",
    "plt.ylabel('Predictions Made')\n",
    "plt.grid(True)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
